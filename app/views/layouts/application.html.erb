<!DOCTYPE html>
<html>
<head>
  <title>Kakaoreview</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container-fluid">

    <%= render "/layout/nav" %>

    <div class="row-fluid">
      <div class="span2">
        <!-- Sidebar contents -->
        <%= yield :sidebar %>
      </div>
      <div id="content_box" class="span10">
        <!-- Contents -->
        <%= yield %>
      </div>
    </div>
  </div>

</body>
</html>
